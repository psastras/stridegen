{
  "name": "stridegen",
  "version": "0.0.15",
  "description": "Swagger documentation generation.",
  "scripts": {
    "start": "node server.js",
    "build": "cross-env NODE_ENV=production webpack -p --progress --config webpack.config.js",
    "postinstall": "npm run build",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register --reporter mocha-circleci-reporter test/*.js",
    "lint": "eslint src"
  },
  "bin": { "stridegen": "./stridegen.sh" },
  "repository": {
    "type": "git",
    "url": "https://github.com/psastras/stridegen.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "swagger",
    "swagger-ui",
    "api",
    "documentation",
    "openapi",
    "webpack"
  ],
  "author": "Paul Sastrasinh (http://github.com/psastras)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psastras/stridegen/issues"
  },
  "homepage": "https://github.com/psastras/stridegen",
  "dependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "cssnano": "3.7.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "highlight.js": "9.4.0",
    "html-webpack-plugin": "2.21.0",
    "json-loader": "0.5.4",
    "json-refs": "2.1.6",
    "node-sass": "6.0.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "3.2.1",
    "style-loader": "0.13.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "json-format": "0.1.2",
    "material-colors": "1.0.0",
    "open-sans-fontface": "1.4.0",
    "optimize-css-assets-webpack-plugin": "1.3.0",
    "react": "15.1.0",
    "react-addons-css-transition-group": "15.1.0",
    "react-dom": "15.1.0",
    "react-highlight": "0.8.0",
    "react-inline-css": "2.1.0",
    "react-lite": "0.15.14",
    "react-markdown": "2.3.0",
    "react-router": "2.0.0",
    "shelljs": "0.7.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "mocha": "2.5.3",
    "mocha-circleci-reporter": "0.0.1"
  }
}
